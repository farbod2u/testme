<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ir.saeed.spring.entities.Address" table="t_address">
		<id name="id" type="integer" column="f_id">
			<generator class="native"></generator>
		</id>
		<property name="city" type="string" column="f_city"
			not-null="true" />
		<property name="province" type="string" column="f_province"
			not-null="true" />
	</class>
</hibernate-mapping>